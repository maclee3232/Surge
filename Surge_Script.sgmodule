cron "19 10 * * *" script-path=https://raw.githubusercontent.com/jkgtw/Surge/master/JS/shopee-watercheckin.js, wake-system=1, timeout=30
cron "25 14 * * *" script-path=https://raw.githubusercontent.com/jkgtw/Surge/master/JS/shopee-watercheckin.js, wake-system=1, timeout=30
cron "39 18 * * *" script-path=https://raw.githubusercontent.com/jkgtw/Surge/master/JS/shopee-watercheckin.js, wake-system=1, timeout=30

